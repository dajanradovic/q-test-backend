{
	"name": "q/test-project",
	"version": "1.0.0",
	"description": "Q - test project",
	"authors": [
		{
			"name": "Dajan Radovic",
			"email": "dajan.radovic@gmail.com"
		}
	],
	"type": "project",
	"require": {
		"php": "^7.4|^8.0",
		"eftec/bladeone": "^4.1",
		"guzzlehttp/guzzle": "^7.4",
		"vlucas/phpdotenv": "^5.3"
	},
	"require-dev": {
		"fakerphp/faker": "^1.9",
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/"
			},
			"files": [
				"app/Helpers/helpers.php"
			]
		},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true,
		"process-timeout": 0
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
